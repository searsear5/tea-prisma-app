generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model customer {
  customer_id Int    @id @default(autoincrement())
  username    String
  userpass    String
  name        String
  lastname    String
  email       String
  address     String
  phone       String
}
